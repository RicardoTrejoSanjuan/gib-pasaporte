        "pg": "^7.12.0",
        "ts-node": "^8.3.0",
        "typescript": "^3.4.5",
        "@instana/collector": "^1.67.0"
    },
    "deploy": {
        "configmaps": [
            "proxy",
            "banxico",
            "conexion-postgres"
        ],
        "volumes": [{
