    "license": "ISC",
    "bugs": {
        "url": "https://gitlab.com/multiva-codi/codi-08-consultamensajescobrocomprador/issues"
    },
    "homepage": "https://gitlab.com/multiva-codi/codi-08-consultamensajescobrocomprador#readme",
    "devDependencies": {
        "jest": "^24.7.1",
        "nock": "^10.0.6",
        "supertest": "^4.0.2",
        "ts-jest": "^24.0.2",
        "tslint": "^5.16.0"
    },
    "dependencies": {
        "@babel/types": "^7.4.4",
        "@types/express": "^4.16.1",
        "@types/got": "^9.4.4",
        "@types/jest": "^24.0.12",
        "@types/nock": "^10.0.1",
        "@types/node": "^11.13.8",
        "@types/pg": "^7.4.14",
        "@types/supertest": "^2.0.7",
        "ajv": "^6.10.0",
        "async-middleware": "^1.2.1",
        "banca-movil-schemas": "^1.2.44",
